# Show or hide the services section in the dashboard (yes/no)
SHOW_SERVICES=yes
# If set, this URL will be shown in place of the services list
CUSTOM_SERVICES_URL=

# Default library to show in carousel
# Use "all" for random-all, or specify library numbers separated by commas
DEFAULT_LIBRARY=all

# Show or hide quick access
QUICK_ACCESS_ENABLED=
QA_PLEX_ENABLED=
QA_AUDIOBOOKSHELF_ENABLED=
QA_TAUTULLI_ENABLED=
QA_OVERSEERR_ENABLED=
QA_JELLYSEERR_ENABLED=

# Library carousel settings (comma-separated list of library IDs that should show carousels)
# Leave empty to show carousels for all libraries, or specify library IDs like "1,2,3"
LIBRARY_CAROUSELS=

# Library carousel tab order (comma-separated list of library IDs in desired order)
# Leave empty to use default Plex order, or specify custom order like "3,1,2"
LIBRARY_CAROUSEL_ORDER=


# Rate limiting settings
RATE_LIMIT_LOGIN_LOCKOUT_DURATION=3600
RATE_LIMIT_IP_SUSPICIOUS_THRESHOLD=20
RATE_LIMIT_IP_BAN_THRESHOLD=50


# Rate limiting configuration
RATE_LIMIT_SETTINGS_ENABLED=yes
RATE_LIMIT_MAX_LOGIN_ATTEMPTS=5
RATE_LIMIT_MAX_FORM_SUBMISSIONS=2


# Audit logging
ENABLE_AUDIT_LOGGING=yes
AUDIT_LOG_RETENTION_DAYS=90

# IP Management settings
IP_MANAGEMENT_ENABLED=no
IP_WHITELIST=
IP_BLACKLIST=

# Adaptive restart configuration
RESTART_DELAY_SECONDS=15
POSTER_DOWNLOAD_TIMEOUT=300
IMMEDIATE_RESTART_DELAY=5
STANDARD_RESTART_DELAY=15
APP_PORT=10000
