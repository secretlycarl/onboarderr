# For Linux/Docker (/,/data,/mnt/c)
DRIVES=
# For Windows (non-docker)
# DRIVES=E:\,F:\
SERVER_NAME=
ACCENT_COLOR=#d33fbc
PLEX_TOKEN=
# Hashed API key fields (auto-generated when tokens are set)
PLEX_TOKEN_HASH=
PLEX_URL=
ABS_ENABLED=
AUDIOBOOKSHELF_URL=
AUDIOBOOKSHELF_TOKEN=
# Hashed API key fields (auto-generated when tokens are set)
AUDIOBOOKSHELF_TOKEN_HASH=
AUDIOBOOKS_ID=
LIBRARY_IDS=
LIBRARY_NAMES=
DISCORD_WEBHOOK=
DISCORD_USERNAME=
DISCORD_AVATAR=
DISCORD_COLOR=
DISCORD_NOTIFY_PLEX=1 # 1=true 0=false
DISCORD_NOTIFY_ABS=1
ONBOARDERR_URL=
# Set Service URLS. If left blank the item will not be shown in the list.
PLEX=https://app.plex.tv
LIDARR=http://localhost:8686
RADARR=http://localhost:7878
SONARR=http://localhost:8989
TAUTULLI=http://localhost:8181
QBITTORRENT=http://localhost:8080
IMMICH=http://localhost:2283
PROWLARR=http://localhost:9696
BAZARR=http://localhost:6767
PULSARR=http://localhost:3003
AUDIOBOOKSHELF=https://your-audiobookshelf-domain.com
OVERSEERR=https://your-overseerr-domain.com
JELLYSEERR=https://your-jellyseerr-domain.com
# Show or hide the services section in the dashboard (yes/no)
SHOW_SERVICES=yes
# If set, this URL will be shown in place of the services list
CUSTOM_SERVICES_URL=

# Default library to show in carousel
# Use "all" for random-all, or specify library numbers separated by commas
DEFAULT_LIBRARY=all

# Show or hide quick access
QUICK_ACCESS_ENABLED=
QA_PLEX_ENABLED=
QA_AUDIOBOOKSHELF_ENABLED=
QA_TAUTULLI_ENABLED=
QA_OVERSEERR_ENABLED=
QA_JELLYSEERR_ENABLED=

# Library carousel settings (comma-separated list of library IDs that should show carousels)
# Leave empty to show carousels for all libraries, or specify library IDs like "1,2,3"
LIBRARY_CAROUSELS=

APP_PORT=12345

# Rate limiting settings
RATE_LIMIT_LOGIN_ATTEMPTS=5
RATE_LIMIT_LOGIN_LOCKOUT_DURATION=3600
RATE_LIMIT_FORM_SUBMISSIONS=1
RATE_LIMIT_PIN_ATTEMPTS=5
RATE_LIMIT_ACCESS_CODE_ATTEMPTS=10
RATE_LIMIT_IP_SUSPICIOUS_THRESHOLD=20
RATE_LIMIT_IP_BAN_THRESHOLD=50

# PIN code settings
ENABLE_PIN_CODES=no
PIN_CODE_LENGTH=6
PIN_CODE_MAX_ATTEMPTS=5
PIN_CODE_LOCKOUT_DURATION=3600
ADMIN_PIN_EXPIRY=never
PUBLIC_PIN_EXPIRY=never

# User group settings
ENABLE_USER_GROUPS=no
USER_GROUP_ACCESS_CODE_LENGTH=6
USER_GROUP_PIN_CODE_LENGTH=6
USER_GROUP_EXPIRY_OPTIONS=never,1day,7days,30days,90days

# Audit logging
ENABLE_AUDIT_LOGGING=yes
AUDIT_LOG_RETENTION_DAYS=90
LOG_FAILED_LOGIN_ATTEMPTS=yes
LOG_FORM_SUBMISSIONS=yes
LOG_IP_ADDRESSES=yes
NOTIFY_ADMIN_ON_SECURITY_EVENTS=yes

# Session settings
SESSION_TIMEOUT_MINUTES=480
MAX_CONCURRENT_SESSIONS=3
ENABLE_SESSION_FINGERPRINTING=yes

# Public access settings
ENABLE_PUBLIC_ACCESS=no
BYPASS_SITE_PASSWORD=no
ADMIN_ACCESS_ALWAYS_PROTECTED=yes

# IP Management settings
IP_MANAGEMENT_ENABLED=no
IP_WHITELIST=
IP_BLACKLIST=
